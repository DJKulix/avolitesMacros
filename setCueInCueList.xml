<?xml version="1.0" encoding="utf-8"?>
<!-- Sets cue in cue list according to pressed button on launchpad and fires it at 100% -->
<avolites.macros>
<macro id="UserMacro.Cue1_1" name = "CueList 1.1_v2">
    <sequence>
      <step>Playbacks.SetPlaybackLevel("Location=Playbacks,1,1", level:1)</step>
      <step>CueLists.SetNextCue(userNumber:2, 1.0)</step>
      <step>CueLists.Play(userNumber:2)</step>
    </sequence>
  </macro>

